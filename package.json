{
  "name": "n8n-nodes-airbnb-scraper",
  "version": "0.1.0",
  "description": "n8n node to scrape Airbnb listing data using the Airbnb Analyzer API",
  "keywords": [
    "n8n-community-node-package",
    "airbnb",
    "scraper",
    "listing",
    "real-estate"
  ],
  "license": "MIT",
  "homepage": "https://shortrentals.ai",
  "author": {
    "name": "ShortRentals AI",
    "email": "support@shortrentals.ai"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ShortRentals-AI/n8n-nodes-airbnb-scraper.git"
  },
  "bugs": {
    "url": "https://github.com/ShortRentals-AI/n8n-nodes-airbnb-scraper/issues"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials --ext .ts",
    "lintfix": "eslint nodes credentials --ext .ts --fix",
    "check": "npx @n8n/scan-community-package .",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AirbnbScraperApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/AirbnbScraper/AirbnbScraper.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.56.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.22.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
